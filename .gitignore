# Ignore directories generated by Composer.
/drush/Commands/contrib
/web/core
/web/libraries
/web/modules/contrib
/web/profiles/contrib
/web/themes/contrib
/web/autoload.php
/vendor

# Ignore sensible data files.
/web/sites/*/settings.local.php
/web/sites/*/settings.prod.php

# Ignore Drupal's file directory.
/web/sites/*/files/*

# Ignore Drupal's test site.
/web/sites/simpletest

# Ignore IDE files
.idea
.vscode

# Misc
*.log
*.DS_Store
node_modules
Thumbs.db
.env
/web/themes/custom/*/dist
backups
old
*.sql.gz
# Ignore ddev/development services file.
/web/sites/*/development.services.yml
/web/sites/*/settings.ddev.php
mutagen.yml
mutagen.yml.lock

# Vim generated files #
######################
*.un~

# ignore twig files outside web.
/storage/php

# Ignore ddev secrets.
.ddev/docker-compose.secrets.yaml

